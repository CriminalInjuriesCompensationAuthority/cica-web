{
    "name": "cica-web",
    "version": "2.3.2",
    "private": true,
    "scripts": {
        "start:dev": "nodemon -L --inspect=0.0.0.0:9229 -e .css,.js,.json,.njk --ignore public/dist/ --ignore src/js/scripts.babel.generated.js --exec npm run buildRun:dev",
        "start": "node ./bin/www",
        "sass": "node-sass src/sass/all.scss public/stylesheets/all.css && node-sass src/sass/all-ie8.scss public/stylesheets/all-ie8.css",
        "test": "npm run test:node && npm run test:dom",
        "test:dom": "jest --config=jest.config.jsdom.js",
        "test:node": "jest --config=jest.config.js",
        "coverage": "jest --coverage",
        "build:dev": "npm run copy:css && npm run copy:js && npm run babel && npm run webpack:dev",
        "build": "npm run copy:css && npm run copy:js && npm run babel && npm run webpack",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "buildRun:dev": "npm run build:dev && node ./bin/www",
        "copy:css": "rm -rf /public/dist/css || true && mkdir -p public/dist/css && cp -r ./src/modules/**/*.css public/dist/css/",
        "copy:js": "rm -rf /public/dist/js || true && mkdir -p public/dist/js && cp -r ./src/modules/**/vendor/*.js public/dist/js/",
        "webpack:dev": "webpack --config webpack.config.js",
        "webpack": "webpack --config webpack.config.prod.js",
        "babel": "babel src/js/scripts.js -o src/js/scripts.babel.generated.js"
    },
    "dependencies": {
        "@ministryofjustice/frontend": "0.0.17-alpha",
        "client-sessions": "github:CriminalInjuriesCompensationAuthority/node-client-sessions#v0.9.0",
        "cookie-parser": "~1.4.3",
        "csurf": "^1.10.0",
        "debounce": "^1.2.0",
        "debug": "~2.6.9",
        "express": "~4.16.0",
        "got": "^9.6.0",
        "govuk-frontend": "^3.10.2",
        "helmet": "^3.15.0",
        "js-cookie": "^2.2.1",
        "lodash.merge": "^4.6.1",
        "moment-timezone": "^0.5.33",
        "morgan": "~1.9.0",
        "nanoid": "^2.1.10",
        "nunjucks": "^3.2.1",
        "q-transformer": "github:CriminalInjuriesCompensationAuthority/q-transformer#v4.0.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.12.10",
        "@babel/core": "^7.12.10",
        "@babel/plugin-transform-runtime": "^7.12.10",
        "@babel/preset-env": "^7.12.11",
        "axios": "^0.19.2",
        "babel-loader": "^8.2.2",
        "core-js": "^3.8.3",
        "custom-event": "^1.0.1",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-config-prettier": "^6.9.0",
        "eslint-plugin-import": "^2.19.1",
        "eslint-plugin-jest": "^23.3.0",
        "eslint-plugin-prettier": "^3.1.2",
        "fs-extra": "^8.1.0",
        "husky": "^4.0.3",
        "jest": "^26.6.3",
        "lint-staged": "^9.5.0",
        "node-sass": "^4.13.1",
        "nodemon": "^1.19.4",
        "prettier": "^1.19.1",
        "q-base-config": "github:CriminalInjuriesCompensationAuthority/q-base-config#v0.10.0",
        "supertest": "^4.0.2",
        "time-convert": "0.0.2",
        "webpack": "^4.46.0",
        "webpack-cli": "^3.3.12"
    },
    "license": "MIT",
    "_m0": {
        "devDependencies": {
            "eslint": "^6.8.0",
            "eslint-config-airbnb-base": "^14.0.0",
            "eslint-config-prettier": "^6.9.0",
            "eslint-plugin-import": "^2.19.1",
            "eslint-plugin-jest": "^23.3.0",
            "eslint-plugin-prettier": "^3.1.2",
            "fs-extra": "^8.1.0",
            "husky": "^4.0.3",
            "jest": "^26.6.3",
            "lint-staged": "^9.5.0",
            "prettier": "^1.19.1"
        },
        "blocks": [
            ".eslintignore",
            ".eslintrc.js",
            ".huskyrc.js",
            ".lintstagedrc.js",
            ".prettierrc.js",
            ".gitignore",
            "jest.config.js"
        ],
        "files": [
            ".editorconfig",
